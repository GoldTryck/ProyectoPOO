      </HBox>
      <VBox fx:id="dataPane" alignment="CENTER" prefHeight="600.0" prefWidth="900.0">
         <children>
            <ImageView fitHeight="351.0" fitWidth="424.0" opacity="0.21" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/background.png" />
