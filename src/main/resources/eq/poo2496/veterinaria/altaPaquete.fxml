<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:controller="eq.poo2496.veterinaria.controller.AltaPaqueteController" prefHeight="590.0" prefWidth="900.0" spacing="10.0" styleClass="progress" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="137.0" prefWidth="622.0">
         <children>
            <Label alignment="CENTER" styleClass="container" text="Alta de Paquetes">
               <font>
                  <Font name="Tahoma Bold" size="40.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <VBox prefHeight="491.0" prefWidth="622.0" spacing="10.0">
         <children>
            <HBox prefHeight="30.0" prefWidth="576.0">
               <children>
                  <TextField fx:id="nombre" prefHeight="29.0" prefWidth="224.0" promptText="Nombre Paquete" />
               </children>
            </HBox>
            <Label alignment="BOTTOM_LEFT" prefHeight="33.0" prefWidth="576.0" text="Seleccione los servicios para agregar al paqute:">
               <font>
                  <Font name="Tahoma Bold" size="15.0" />
               </font>
            </Label>
            <TableView fx:id="paquete" prefHeight="209.0" prefWidth="576.0">
              <columns>
                  <TableColumn fx:id="idPaquete" prefWidth="132.0" text="Id" />
                <TableColumn fx:id="nombrePaquete" prefWidth="205.5" text="Nombre" />
                <TableColumn fx:id="precioPaquete" prefWidth="237.5" text="Precio" />
              </columns>
            </TableView>
            <HBox alignment="CENTER" prefHeight="65.0" prefWidth="576.0">
               <children>
                  <Button fx:id="register" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="Registrar" />
               </children></HBox>
         </children>
         <VBox.margin>
            <Insets left="150.0" right="150.0" />
         </VBox.margin>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
